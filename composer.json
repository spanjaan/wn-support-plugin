{
    "name": "spanjaan/support",
    "type": "winter-plugin",
    "description": "A simple ticket system for WinterCMS",
    "license": "MIT",
    "authors": [
        {
            "name": "S.P. Anjaan",
            "email": "spanjaan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ezyang/htmlpurifier": "^4.7",
        "hashids/hashids": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "SpAnjaan\\Support\\": "plugins/spanjaan/Support/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
